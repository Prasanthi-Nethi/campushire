  <style>
    .panel-box {
      display: table;
      table-layout: fixed;
      width: 100%;
      height: 30px;
      text-align: center;
      border: medium none;
    }
    .bg-success {
      background-color: #A3C86D !important;
      color: #FFF !important;
    }
    .bg-warning {
      background-color: orange !important;
      color: #FFF !important;
    }
    .bg-light {
      background-color: lightblue !important;
      color: #FFF !important;
    }
    .bg-lightgreen {
      background-color: lightgreen !important;
      color: #FFF !important;
    }
    .bg-default {
      background-color: grey !important;
      color: #FFF !important;
    }
    .bg-pink {
      background-color: pink !important;
      color: #FFF !important;
    }
    .bg-tomato {
      background-color: tomato !important;
      color: #FFF !important;
    }
    .bg-dodgerblue {
      background-color: dodgerblue !important;
      color: #FFF !important;
    }
    .bg-violet {
      background-color: violet !important;
      color: #FFF !important;
    }
    .success-text {
      color: #82b33a;
    }
    .bg-danger {
      background-color: #FF7857 !important;
      color: #FFF !important;
    }
    .danger-text {
      color: #d15b3d;
    }
    .panel-box .panel-icon {
      display: table-cell;
      padding: px;
      width: 1%;
      vertical-align: top;
      border-radius: 0px;
      text-align: center;
      /*border-bottom: 1px solid #fcfcfc;
      border-top: 1px solid #fcfcfc;
      border-right: 1px solid #fcfcfc;*/
    }
    .panel-box .size-h1 {
      margin-top: 7px;
    }
    .panel-box .fa {
      margin-top: 9px;
    }
    .bg-lovender {
      background-color: #8075C4 !important;
      color: #FFF !important;
    }
    .lovender-text {
      color: #6a5fb1;
    }
    .capitalize {
      text-transform: capitalize;
    }
    .uppercase {
      text-transform: uppercase;
    }


    h1{
  padding:50px;
  background-color:skyblue;
  font-size:50px;
  width:100%;
}
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;

}

td, th {
  border: 3px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}

